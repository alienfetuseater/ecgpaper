{
	"compilerOptions": {
		// "strict": true,
		"outDir": "./dist/",
		"noImplicitAny": true,
		"module": "es2015",
		"target": "es6",
		"allowJs": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"allowSyntheticDefaultImports": true,
		"baseUrl": "./",
		"paths": {
			"@/*": ["src/*"]
		}
	}
}
